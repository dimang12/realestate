@using System.Collections
@model IEnumerable<RealEstate.Models.Listing>
<ul class="list-unstyled mt-3 listing-list">
    @{
        var listings = ViewData["listing"];
        foreach (var ls in (IEnumerable<Listing>) listings )
        {

            <li class="row mb-3">
                <div
                    style="background:url(/img/thumb/@ls.Image); background-position: center; background-size: cover; "
                    class="col-md-12 position-relative thumb-md-height">
                    <div class="rate text-warning">
                        <i class="icon-star"></i>
                        <i class="icon-star"></i>
                        <i class="icon-star"></i>
                        <i class="icon-star"></i>
                        <i class="icon-star"></i>
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="row mt-2">
                        <div class="col-md-6">
                            <h5 class="mt-0 mb-1">$ @ls.Price</h5>
                        </div>
                        <div class="col-md-6 text-md-right">
                            <i class="icon-phone "></i> (510) 988 9872
                        </div>
                    </div>
                    <p class="m-0 font-14">@ls.Address</p>
                    <p>By:<a href="" class="font-14"> @ls.Company </a></p>
                    
                </div>
            </li>
        }
    }
</ul>